{
  "name": "rentify-backend",
  "version": "1.0.0",
  "description": "Car rental backend service built with TypeScript and Supabase",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint src/**/*.ts",
