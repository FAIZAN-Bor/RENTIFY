    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "i18n": "^0.15.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
